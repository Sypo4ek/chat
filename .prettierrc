{
  "indent": 2,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "semi": false,
  "trailingComma": "none",
  "singleQuote": true,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "htmlWhitespaceSensitivity": "strict",
  "proseWrap": "always",
  "embeddedLanguageFormatting": "auto",
  "quoteProps": "as-needed"
}
